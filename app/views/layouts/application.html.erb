<!DOCTYPE html>
<html>
<head>
  <title>Kristen and Todd are getting married!</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/jquery.slick/1.3.6/slick.css"/>



</head>
<body class="<%= controller.controller_name %> <%= controller.action_name %>">
  <% if (warning=flash[:warning]).present? || notice.present? || alert.present? %>
    <div class="notifications-container text-center">
      <% if warning.present? %>
        <div class="alert alert-danger">
          <%= warning %>
        </div>
      <% end %>
      <% if alert.present? %>
        <div class="alert alert-warning">
          <%= alert %>
        </div>
      <% end %>
      <% if notice.present? %>
        <div class="alert alert-success">
          <%= notice %>
        </div>
      <% end %>
    </div>
  <% end %>
    <%= yield %>

  <%= javascript_include_tag 'application' %>

  <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js">
  </script>
  <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js">
  </script>
  <script type="text/javascript" src="//cdn.jsdelivr.net/jquery.slick/1.3.6/slick.min.js"/></script>

</body>
</html>
